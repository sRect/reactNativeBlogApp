{
  "name": "blogApp",
  "version": "0.0.7",
  "private": true,
  "scripts": {
    "postversion": "react-native-version",
    "preandroid": "node ./scripts/checkEnvAmapKey .env.local && cat .env.local >> .env.development",
    "android": "ENVFILE=.env.development react-native run-android",
    "postandroid": "node ./scripts/revertEnvFile .env.development .env.local",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "devtools": "npx react-devtools",
    "prebuild:android": "node ./scripts/checkEnvAmapKey .env.local && cat .env.local >> .env.production",
    "build:android": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "postbuild:android": "node ./scripts/revertEnvFile .env.production .env.local",
    "prebuild:android:aab": "node ./scripts/checkEnvAmapKey .env.local && cat .env.local >> .env.production",
    "build:android:aab": "cd android && ENVFILE=.env.production ./gradlew bundleRelease",
    "postbuild:android:aab": "node ./scripts/revertEnvFile .env.production .env.local",
    "preinstall:android": "node ./scripts/checkEnvAmapKey .env.local && cat .env.local >> .env.production",
    "install:android": "ENVFILE=.env.production npx react-native run-android --variant=release",
    "postinstall:android": "node ./scripts/revertEnvFile .env.production .env.local",
    "gradlew:clean": "cd android &&  ENVFILE=.env.production ./gradlew clean",
    "assets:link": "react-native-asset"
  },
  "dependencies": {
    "@ant-design/icons-react-native": "^2.3.2",
    "@ant-design/react-native": "^5.0.3",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/geolocation": "^3.0.5",
    "dayjs": "^1.11.9",
    "github-syntax-light": "^0.5.0",
    "react": "18.2.0",
    "react-native": "0.71.2",
    "react-native-amap3d": "^3.2.4",
    "react-native-asset": "^2.0.1",
    "react-native-config": "^1.5.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-webview": "^11.26.1",
    "react-native-wechat-lib": "3.0.4",
    "react-router-native": "^6.8.0",
    "styled-components": "^5.3.6",
    "wgs2mars": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-import": "^1.13.6",
    "chalk": "4.1.2",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-devtools": "^4.27.1",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "shelljs": "^0.8.5",
    "typescript": "4.8.4",
    "uri-scheme": "^1.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
